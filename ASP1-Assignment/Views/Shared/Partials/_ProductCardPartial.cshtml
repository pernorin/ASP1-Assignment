@model ProductModel

<div class="col">
	<div class="card">
		<img src="~/@Model.ImageUrl" class="card-img-top" alt="">
		@*<img src="@Model.ImageUrl" class="card-img-top" alt="" />*@
		<div class="card-body text-start">
			<h3 class="card-title">@Model.Name</h3>
			<p class="card-text text-light">
				$@Model.Price <span><i class="bi bi-cart-plus float-end"></i></span>
			</p>
		</div>
	</div>
</div>